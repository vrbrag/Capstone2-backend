{
   "$schema": "http://json-schema.org/draft-07/schema#",
   "$id": "http://example.com/example.json",
   "type": "object",
   "properties": {
      "firstName": {
         "type": "string",
         "minLength": 1,
         "maxLength": 30
      },
      "lastName": {
         "type": "string",
         "minLength": 1,
         "maxLength": 30
      },
      "email": {
         "type": "string",
         "minLength": 6,
         "maxLength": 60,
         "format": "email"
      },
      "age": {
         "type": "integer",
         "minimum": 1,
         "maximum": 99
      },
      "weight": {
         "type": "integer",
         "minimum": 1,
         "maximum": 600
      },
      "height": {
         "type": "integer",
         "minimum": 1,
         "maximum": 99
      },
      "gender": {
         "type": "string"
      },
      "pal": {
         "type": "number"
      },
      "goalWeight": {
         "type": "string"
      },
      "password": {
         "type": "string",
         "minLength": 5,
         "maxLength": 20
      }
   },
   "additionalProperties": false,
   "required": []
}