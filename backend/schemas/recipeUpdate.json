{
   "$schema": "http://json-schema.org/draft-07/schema#",
   "$id": "http://our.recipe.com/recipeUpdate.schema.json",
   "type": "object",
   "properties": {
      "title": {
         "type": "string",
         "minLength": 1,
         "maxLength": 65
      },
      "cuisine": {
         "type": "string"
      },
      "ingredients": {
         "type": "string",
         "minimum": 1
      },
      "instructions": {
         "type": "string"
      },
      "avgCal": {
         "type": "number"
      },
      "notes": {
         "type": "string"
      },
      "username": {
         "type": "string",
         "minLength": 1,
         "maxLength": 30
      }
   },
   "additionalProperties": false,
   "required": [
      "title",
      "username"
   ]
}